{
    "name": "leadingfellows/config-parser",
    "description": "Configuration parser/merger",
    "keywords": ["dotenv", "yaml", "configuration"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "leadingfellows\\utils\\": "src/"
        }
    },
    "require": {
		"symfony/yaml": ">=3.2",
        "grasmash/yaml-expander": "^1 || ^2 || ^3",
        "josegonzalez/dotenv": ">=3.2"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3.12",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.4"
    }
}
